{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=${DB_HOST};Database=${DB_NAME};User=${DB_USER};Password=${DB_PASSWORD};"
  },
  "Aws": {
    "Region": "${AWS_REGION}",
    "SqsQueueUrl": "${AWS_SQS_QUEUE_URL}"
  }
}
